<div>
    <div class="container" style="text-align: right;">
        {{username}}
    </div>
    <div class="container" style="font-size: large;">
        {{fullname}}
    </div>
    <div class="row">
        <div class="col-4">
            <p>Age: {{age}}</p>
            <p>Genres: {{genres}}</p>
        </div>
        <div class="col-4">
            <p>email: {{email}}</p>
        </div>
    </div>
    <div class="container">
        <p>My Books List:</p>
    </div>
    <div class="d-flex flex-wrap">
        {{#each bookslist}}
        <div style="width: 200px;">
            <img src="{{bookimage}}" alt="" style="width: 200px;">
            <p>{{booktitle}}</p>
            {{#if rating === 1}}
                ★☆☆☆☆
            {{#else if rating === 2}}
                ★★☆☆☆
            {{#else if rating === 3}}
                ★★★☆☆
            {{#else if rating === 4}}
                ★★★★☆
            {{#else if rating === 5}}
                ★★★★★
            {{/if}}
        </div>
        {{/each}}
    </div>
</div>
    <script>
        var review = bookslist
    </script>